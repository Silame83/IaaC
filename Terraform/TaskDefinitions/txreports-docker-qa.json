[
  {
    "name": "txreports-docker-qa",
    "image": "639716861848.dkr.ecr.eu-west-1.amazonaws.com/newtxreports:commitid-a4ef2d9",
    "memory": 256,
    "essential": true,
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "/ecs/txreports-docker-qa",
        "awslogs-region": "eu-west-3",
        "awslogs-create-group": "true"
      }
    },
    "portMappings": [
      {
        "containerPort": 8080,
        "hostPort": 0,
        "protocol": "tcp"
      }
    ],
    "command": [
      "npm",
      "start"
    ],
    "environment": [
      {
        "name": "APPLICATION_NAME",
        "value": "COLU-TXREPORTS"
      },
      {
        "name": "ENV",
        "value": "QA"
      }
    ],
    "healthCheck": {
      "retries": 3,
      "command": [
        "CMD-SHELL",
        "curl -f http://localhost:8080/brokertest || exit 1"
      ],
      "timeout": 5,
      "interval": 30,
      "startPeriod": 30
    },
    "dockerLabels": {
      "app-name": "txreports"
    },
    "name": "txreports"
  }
]